{
    "name": "dino/dompdf-module",
    "type": "library",
    "description": "A Zend Framework 2 module for incorporating DOMPDF support.",
    "keywords": ["pdf","dompdf", "zf2"],
    "homepage": "http://raymondkolbe.com",
    "license": "MIT",
    "authors": [
        {
        "name": "Raymond J. Kolbe",
        "email": "rkolbe@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.29",
        "zendframework/zendframework": "^2.1.6",
        "dompdf/dompdf": "^0.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.27"
    },
    "autoload": {
        "psr-0": {
            "DOMPDFModule": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "DOMPDFModuleTest": "tests/"
        }
    }
}
